{
  "name": "sky-server",
  "version": "1.0.0",
  "description": "sky server",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "prisma:generate:test": "dotenv -e .env.test -- npx prisma migrate dev --name init",
    "prisma:generate:dev": "dotenv -e .env.development -- npx prisma migrate dev --name init",
    "prisma:generate:prod": "dotenv -e .env.production -- npx prisma migrate dev --name init",
    "test": "dotenv -e .env.test cross-env NODE_ENV=test jest --detectOpenHandles"
  },
  "author": "gianicola jara",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf-csrf": "^3.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.5",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/expect": "^1.20.4",
    "@types/express": "^5.0.0",
    "@types/express-mysql-session": "^3.0.6",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "prisma": "^6.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2"
  }
}
